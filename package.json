{
  "name": "qqwry.js",
  "version": "0.0.1",
  "description": "Nest.js IP library based on qqwry.dat",
  "keywords": [
    "IP",
    "IP Database",
    "IP Library",
    "qqwry",
    "qqwry.dat",
    "NestJS",
    "MySQL",
    "Fuyun"
  ],
  "author": {
    "name": "Fuyun",
    "url": "https://www.ifuyun.com"
  },
  "private": false,
  "license": "MIT",
  "homepage": "https://www.ifuyun.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ifuyun/qqwry.js"
  },
  "bugs": {
    "url": "https://github.com/ifuyun/qqwry.js/issues"
  },
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "cross-env ENV=development nest start",
    "start:dev": "cross-env ENV=development nest start --watch",
    "start:debug": "cross-env ENV=development nest start --debug --watch",
    "start:prod": "cross-env ENV=production node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.3",
    "@nestjs/common": "^10.4.1",
    "@nestjs/config": "^3.2.3",
    "@nestjs/core": "^10.4.1",
    "@nestjs/platform-express": "^10.4.1",
    "@nestjs/schedule": "^4.1.0",
    "@nestjs/sequelize": "^10.0.1",
    "cron": "^3.1.7",
    "iconv-lite": "^0.6.3",
    "lodash": "^4.17.21",
    "mysql2": "^3.11.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "sequelize": "^6.37.3",
    "sequelize-typescript": "^2.1.6"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.4",
    "@nestjs/schematics": "^10.1.4",
    "@nestjs/testing": "^10.4.1",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash": "^4.17.7",
    "@types/node": "^22.4.1",
    "@types/supertest": "^6.0.2",
    "eslint": "^9.9.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.4",
    "ts-jest": "^29.2.4",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4",
    "typescript-eslint": "^8.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
